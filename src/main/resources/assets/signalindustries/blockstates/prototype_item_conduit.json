{
  "multipart": [
    {
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_base"
      }
    },
    {
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_base"
      },
      "when": {
        "mode": "SPLIT"
      }
    },
    {
      "when": {
        "AND": [
          {"up": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_up"
      }
    },
    {
      "when": {
        "AND": [
          {"up": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_up"
      }
    },

    {
      "when": {
        "AND": [
          {"down": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_down"
      }
    },
    {
      "when": {
        "AND": [
          {"down": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_down"
      }
    },

    {
      "when": {
        "AND": [
          {"north": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_north"
      }
    },
    {
      "when": {
        "AND": [
          {"north": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_north"
      }
    },

    {
      "when": {
        "AND": [
          {"south": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_south"
      }
    },
    {
      "when": {
        "AND": [
          {"south": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_south"
      }
    },

    {
      "when": {
        "AND": [
          {"east": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_east"
      }
    },
    {
      "when": {
        "AND": [
          {"east": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_east"
      }
    },

    {
      "when": {
        "AND": [
          {"west": "true"},{"mode": "SPLIT"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/split/conduit_west"
      }
    },
    {
      "when": {
        "AND": [
          {"west": "true"},{"mode": "RANDOM"}
        ]
      },
      "apply": {
        "model": "signalindustries:conduit/item/prototype/conduit_west"
      }
    }
  ]
}
